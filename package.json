{
  "name": "@belgattitude/profile-in-a-rush",
  "description": "A personal self challenge hackaton to finally write a CV.",
  "homepage": "https://github.com/belgattitude/profile-in-a-rush",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "version": "2019.10.04",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "export": "next export",
    "start": "next start",
    "clean": "rimraf --no-glob ./.next ./out",
    "typecheck": "tsc -p . --noEmit",
    "format": "prettier --write \"./**/*.{ts,tsx,css,scss,js,json}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git diff --name-only HEAD^ | xargs git add",
      "pre-push": "tsc -p . --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md,mdx,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
