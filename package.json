    "framer-motion": "3.3.0",
    "ky": "^0.26.0",
    "next": "10.0.6",
    "next-transpile-modules": "^6.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-lazyload": "^3.1.0",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0",
    "sharp": "^0.27.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.1.2",
    "@next/bundle-analyzer": "^10.0.6",
