{
  "name": "@belgattitude/profile-in-a-rush",
  "description": "A personal self challenge hackathon to finally write a CV.",
  "homepage": "https://github.com/belgattitude/profile-in-a-rush",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "version": "2021.04.05",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build",
    "analyze": "cross-env ANALYZE=true next build",
    "export": "next export",
    "start": "next start",
    "clean": "rimraf --no-glob ./.next ./out",
    "typecheck": "tsc -p . --noEmit",
    "format": "prettier --write \"./**/*.{ts,tsx,css,scss,js,json}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --config jest.config.js --color $@",
    "deps:check": "npx npm-check-updates --dep prod,dev,optional",
    "deps:update": "npx npm-check-updates --dep prod,dev,optional -u",
    "deduplicate": "yarn-deduplicate yarn.lock",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc -p . --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md,mdx,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "11.1.0",
    "@sindresorhus/is": "^4.0.1",
    "abort-controller": "^3.0.0",
    "abortcontroller-polyfill": "^1.7.3",
    "date-fns": "^2.23.0",
    "framer-motion": "^4.1.17",
    "ky": "^0.28.5",
    "next": "11.1.0",
    "next-transpile-modules": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-lazyload": "^3.2.0",
    "remark-emoji": "^3.0.0",
    "remark-images": "^3.0.0",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@next/bundle-analyzer": "11.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-lazyload": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "lint-staged": "^11.1.2",
    "npm-check-updates": "11.8.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.37.5",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "yarn-deduplicate": "^3.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=14",
    "yarn": ">=1.21.0",
    "npm": "please-use-yarn"
  }
}
