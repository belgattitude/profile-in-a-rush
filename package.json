{
  "name": "@belgattitude/profile-in-a-rush",
  "description": "A personal self challenge hackaton to finally write a CV.",
  "homepage": "https://github.com/belgattitude/profile-in-a-rush",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "version": "2019.10.04",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "clean": "rimraf --no-glob ./.next ./out",
    "typecheck": "tsc -p . --noEmit",
    "format": "prettier --write \"./**/*.{ts,tsx,css,scss,js,json}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git diff --name-only HEAD^ | xargs git add",
      "pre-push": "tsc -p . --noEmit"
    }
  },
  "lint-staged": {
    "*.{json,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@mdx-js/loader": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@next/mdx": "^9.1.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "abort-controller": "^3.0.0",
    "abortcontroller-polyfill": "^1.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "date-fns": "^2.4.1",
    "emotion-theming": "^10.0.19",
    "framer-motion": "^1.6.8",
    "ky": "^0.15.0",
    "next": "9.1.1",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^0.19.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "remark-emoji": "^2.0.2",
    "remark-images": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^4.2.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ]
}
