{
  "presets": [
    [
      "next/babel"
    ]
  ],
  "plugins": [
    [
      "@emotion/babel-plugin"
    ],
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "@config": "./config",
        "@themes": "./src/themes",
        "@core": "./src/core",
        "@components": "./src/components",
        "@public": "./public"
      }
    }]
  ]
}
